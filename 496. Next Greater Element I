class Solution {
    public:
        vector<int> nextGreaterElement(vector<int>& nums1, vector<int>& nums2) {
            int n = nums2.size();
            stack<int> stk;
            vector<int> v(10004,-1);
            for(int i=0;i<n;i++){
                if(stk.empty()){
                    stk.push(nums2[i]);
                    continue;
                }
                while(stk.top()<nums2[i]){
                    v[stk.top()] = nums2[i];
                    stk.pop();
                    if(stk.empty()){
                        break;
                    }
                }
                stk.push(nums2[i]);
            }
            vector<int> ans;
            for(auto it: nums1){
                ans.push_back(v[it]);
            }
            return ans;
        }
    };
